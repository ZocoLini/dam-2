                &lt;condition else=&quot;-suitename JavaApplication1 -testname @{testClass} ${test.class.or.method}&quot; property=&quot;testng.cmd.args&quot; value=&quot;@{testClass}&quot;&gt;
                    &lt;matches pattern=&quot;.*\.xml&quot; string=&quot;@{testClass}&quot;/&gt;
