                        &lt;arg line=&quot;-d ${build.test.results.dir}&quot;/&gt;
                        &lt;arg line=&quot;-listener org.testng.reporters.VerboseReporter&quot;/&gt;
                        &lt;arg line=&quot;${testng.cmd.args}&quot;/&gt;
                    &lt;/customizeDebuggee&gt;
                &lt;/j2seproject3:debug&gt;
            &lt;/sequential&gt;
        &lt;/macrodef&gt;
