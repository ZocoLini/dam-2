                &lt;delete dir=&quot;${build.test.results.dir}&quot; quiet=&quot;true&quot;/&gt;
                &lt;mkdir dir=&quot;${build.test.results.dir}&quot;/&gt;
                &lt;j2seproject3:debug classname=&quot;org.testng.TestNG&quot; classpath=&quot;${debug.test.classpath}&quot;&gt;
                    &lt;customizeDebuggee&gt;
                        &lt;customize2/&gt;
