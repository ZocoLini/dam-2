<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ChoiceBox?>
<GridPane xmlns="http://javafx.com/javafx" fx:id="root"
          xmlns:fx="http://javafx.com/fxml"
          fx:controller="org.lebastudios.aplicacioncompleja.formularios.StageAltaRazaPerroController"
          hgap="10" vgap="5">
    <padding>
        <Insets top="10" left="10" right="10" bottom="10" />
    </padding>
    <columnConstraints>
        <ColumnConstraints prefWidth="175"/>
        <ColumnConstraints minWidth="175" hgrow="ALWAYS"/>
    </columnConstraints>

    <rowConstraints>
        <RowConstraints />
        <RowConstraints />
    </rowConstraints>

    <Label text="%phrase.realizaraltade" maxWidth="Infinity" GridPane.columnIndex="0" GridPane.rowIndex="0" />
    <ChoiceBox maxWidth="Infinity" fx:id="opcionAltaChoiceBox" GridPane.columnIndex="1" GridPane.rowIndex="0" />
    
    <HBox fx:id="opcionPaneContainer" maxWidth="Infinity" maxHeight="Infinity"
          GridPane.columnIndex="0" GridPane.columnSpan="2" GridPane.rowIndex="1" />

</GridPane>
